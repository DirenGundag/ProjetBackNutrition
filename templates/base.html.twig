<html lang="fr">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" 
href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    {% block stylesheets %}
    {% endblock %}
    <title>Blog - {% block title %}{% endblock %}</title>
</head>

<body>

    <nav class="navbar navbar-expand-lg ">
        <a class="navbar-brand" href="{{ path('app_home') }}"><img src="https://media.discordapp.net/attachments/971727360430850062/1113052904471924807/20230529_231800_0000__1_-removebg-preview.png?width=531&height=380" alt="description de l'image" width=150;></a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#mobileMenu" aria-controls="mobileMenu" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon .navbar-light"></span>
        </button>
        <div class="collapse navbar-collapse" id="mobileMenu">
            <ul class="navbar-nav ml-auto mt-2 mt-lg-0">
                {% if app.user %}
                <li class="nav-item">
                    <span class="nav-link">Bienvenue {{ app.user.nom }} {{ app.user.prenom }}!</span>
                </li>
                {% endif %}
                <li class="nav-item {{ app.request.get('_route') == 'homepage' ? 'active' : '' }}">
                    <a class="nav-link" href="{{ path('app_home') }}">Accueil</a>
                </li>
                {% if is_granted('ROLE_ADMIN') %}
                <li class="nav-item {{ app.request.get('_route') == 'app_recette' ? 'active' : '' }}">
                    <a class="nav-link" href="{{ path('app_recette_index') }}">Recette</a>
                </li>
                <li class="nav-item {{ app.request.get('_route') == 'app_regime_index' ? 'active' : '' }}">
                    <a class="nav-link" href="{{ path('app_regime_index') }}">Régime</a>
                </li>
                <li class="nav-item {{ app.request.get('_route') == 'app_allergie' ? 'active' : '' }}">
                    <a class="nav-link" href="{{ path('app_allergie_index') }}">Allergie</a>
                </li>
                <li class="nav-item {{ app.request.get('_route') == 'app_user_index' ? 'active' : '' }}">
                    <a class="nav-link" href="{{ path('app_user_index') }}">User</a>
                </li>
                {% endif %}
                <li class="nav-item {{ app.request.get('_route') == 'app_recette' ? 'active' : '' }}">
                    <a class="nav-link" href="{{ path('app_recette_index') }}">Recette</a>
                </li>
                {% if app.user %}
                <li class="nav-item">
                    <a class="nav-link" href="{{ path('app_logout') }}">Déconnexion</a>
                </li>
                {% else %}
                <li class="nav-item {{ app.request.get('_route') == 'app_login' ? 'active' : '' }}">
                    <a class="nav-link" href="{{ path('app_login') }}">Connexion</a>
                </li>
                <li class="nav-item {{ app.request.get('_route') == 'app_register' ? 'active' : '' }}">
                    <a class="nav-link" href="{{ path('app_register') }}">Inscription</a>
                </li>
                {% endif %}
            </ul>
        </div>
    </nav>

    <div class="container-fluid">



    {% block body %}{% endblock %}
    </div>
    



    <footer class="footer">

<div class="info">
<h4 class="titre">Diététicienne & Nutritionniste</h4>
<p class="adresse">17 avenue de la paix Caen - 14000 </p>
<p class="horaire">Ouvert tous les jours de 11H00 à 14H00 et de 18H00 à 23H00 hors jours fériés.</p>
<p class="tel">Tél: 01 00 00 00 00 </p>
</div>

<div class="container">
<ul class="foote_bottom_ul_amrc">
<a class="espace" href="">Mentions légales</a>
<a class="espace" href="">Politique de confidentialité</a>
<a class="espace" href="">Newsletter</a>
<a class="espace" href="/contact">Contact</a>
</ul>
<!--foote_bottom_ul_amrc ends here-->
<p class="text-center">Copyright @2023 | Designed by Diren Gundag</p>


<!--social_footer_ul ends here-->
</div>

</footer>


<script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
